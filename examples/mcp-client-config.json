{
  "mcpServers": {
    "gautam-mcp-server": {
      "comment": "Gautam MCP Server configuration",
      "command": "node",
      "args": ["dist/index.js"],
      "env": {
        "PORT": "3000"
      }
    },
    "gautam-mcp-server-remote": {
      "comment": "Gautam MCP Server remote configuration",
      "command": "npx",
      "args": ["mcp-remote", "https://gautam-mcp-server.railway.app/mcp"]
    }
  }
} 